; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="HueBound"
run/main_scene="res://Main Menu/main_menu.tscn"
config/features=PackedStringArray("4.3", "Mobile")
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/image="res://UI/jVZD+p.png"
config/icon="res://UI/HueBound_Logo.png"

[autoload]

Global="*res://Global/global.gd"
Settings="*res://Global/Settings.tscn"
SilentWolf="*res://addons/silent_wolf/SilentWolf.gd"
TransitionHandler="*res://Global/TransitionHandler.gd"

[display]

window/size/viewport_width=720
window/size/viewport_height=1280
window/size/always_on_top=true
window/size/window_width_override=360
window/size/window_height_override=640
window/stretch/mode="viewport"
window/handheld/orientation=1

[editor_plugins]

enabled=PackedStringArray("res://addons/silent_wolf/plugin.cfg")

[file_customization]

folder_colors={
"res://Cell/": "yellow",
"res://Game/": "green",
"res://Huey/": "blue",
"res://Main Menu/": "purple"
}

